@model HyMovieRental.Models.Movie

@{
    ViewBag.Title = "Detail";
}

<h2>Detail</h2>

<ul>
    <li>
        Genre: @Model.Genre.Name
    </li>
    <li>
        Release Date: @Model.ReleaseDate.ToLongDateString()
    </li>
    <li>
        Date Added: @Model.DateAdded.ToLongDateString()
    </li>
    <li>
        Number in Stock: @Model.NumberInStock
    </li>
</ul>